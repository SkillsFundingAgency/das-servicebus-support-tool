@model SFA.DAS.Tools.Servicebus.Support.Core.Models.ErrorMessage

<div>

    <table class="table">
        <thead>
            <tr>
                <th>
                    Key
                </th>
                <th>
                    Value
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var kvp in Model.OriginalMessage.UserProperties)
            {
                <tr>
                    <td> @kvp.Key</td>
                    <td> @kvp.Value </td>
                </tr>
            }
        </tbody>
    </table>

</div>