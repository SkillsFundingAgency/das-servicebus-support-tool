{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConfigurationStorageConnectionString": "UseDevelopmentStorage=true;",
  "ConfigNames": "SFA.DAS.Tools.Servicebus.Support.Web",
  "Environment": "LOCAL",
  "Version": "1.0",
  "AllowedHosts": "*",
  "ServiceBusRepoSettings": {
    "ServiceBusConnectionString": "Endpoint=sb://xxxxxxxx.servicebus.windows.net/",
    "QueueSelectionRegex": "[-,_]+error",    
    "PeekMessageBatchSize": 20    
  },
  "ConnectionStrings": {
    "LoggingRedisConnectionString": ""
  },
  "CosmosDb": {
    "Url": "",
    "AuthKey": "",
    "DatabaseName": "QueueExplorer",
    "CollectionName": "Session",
    "Throughput": 400
  },
  "BaseUrl": "localhost:5001",
  "RequiredRole": "Servicebus Admin",
  "APPINSIGHTS_INSTRUMENTATIONKEY": "",
  "ErrorQueueRegex": "[-,_]error[s]*$",
  "Cdn": {
    "Url": ""
  },
  "UserSessionExpirtyHours": 24,
  "UserRefreshSessionIntervalMinutes": 5,
  "RedactPatterns": [
    "(.*SharedAccessKey=)([\\s\\S]+=)(.*)"
  ]
}
